{
  "name": "@alarivan/react-styled-table",
  "version": "0.0.1",
  "description": "React table with styled-components",
  "author": "alarivan",
  "license": "MIT",
  "repository": "alarivan/react-styled-table",
  "main": "dist/index.js",
  "module": "dist/index.modern.js",
  "source": "src/index.ts",
  "engines": {
    "node": ">=12"
  },
  "scripts": {
    "build": "microbundle-crl --tsconfig tsconfig.build.json --no-compress  --format modern,cjs",
    "start": "microbundle-crl --tsconfig tsconfig.build.json watch --no-compress  --format modern,cjs",
    "prepare": "run-s build",
    "test": "run-s test:unit test:lint test:build",
    "test:build": "run-s build",
    "test:lint": "eslint .",
    "test:unit": "cross-env CI=1 react-scripts test --env=jsdom",
    "test:watch": "react-scripts test --env=jsdom",
    "predeploy": "cd example && yarn install && yarn run build",
    "deploy": "gh-pages -d example/build"
  },
  "peerDependencies": {
    "react": "^16.14.0"
  },
  "devDependencies": {
    "@testing-library/dom": "^7.29.0",
    "@testing-library/jest-dom": "^4.2.4",
    "@testing-library/react": "^9.5.0",
    "@testing-library/user-event": "^12.6.0",
    "@types/jest": "^25.1.4",
    "@types/node": "^12.12.38",
    "@types/ramda": "^0.27.34",
    "@types/react": "^16.14.0",
    "@types/react-dom": "^16.9.10",
    "@types/styled-components": "^5.1.7",
    "@typescript-eslint/eslint-plugin": "^4.11.1",
    "@typescript-eslint/parser": "^4.11.1",
    "babel-eslint": "^10.0.3",
    "cross-env": "^7.0.2",
    "eslint": "^7.11.0",
    "eslint-config-prettier": "^6.7.0",
    "eslint-config-standard": "^14.1.0",
    "eslint-config-standard-react": "^9.2.0",
    "eslint-plugin-import": "^2.18.2",
    "eslint-plugin-node": "^11.0.0",
    "eslint-plugin-prettier": "^3.1.1",
    "eslint-plugin-promise": "^4.2.1",
    "eslint-plugin-react": "^7.17.0",
    "eslint-plugin-standard": "^4.0.1",
    "gh-pages": "^2.2.0",
    "jest-styled-components": "^7.0.3",
    "microbundle-crl": "^0.13.10",
    "npm-run-all": "^4.1.5",
    "prettier": "^2.0.4",
    "react": "^16.14.0",
    "react-dom": "^16.14.0",
    "react-scripts": "^4.0.1",
    "typescript": "^4.1.2"
  },
  "files": [
    "dist"
  ],
  "dependencies": {
    "ramda": "^0.27.1",
    "styled-components": "^5.2.1"
  }
}
